{
    "name": "eleventastic",
    "version": "1.0.0",
    "description": "A boilerplate for eleventy websites",
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist/* .cache/*",
        "test": "echo \"Error: no test specified\" && exit 1",
        "postinstall": "mkdir .cache"
    },
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^0.11.1",
        "@11ty/eleventy-cache-assets": "^2.1.0",
        "@11ty/eleventy-navigation": "^0.1.6",
        "@11ty/eleventy-plugin-rss": "^1.0.9",
        "@babel/core": "^7.11.4",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@octokit/rest": "^18.2.0",
        "@popperjs/core": "^2.8.2",
        "babel-loader": "^8.2.2",
        "bootstrap": "^5.0.0-beta2",
        "clean-css": "^4.2.3",
        "critical": "^2.0.3",
        "cssesc": "^3.0.0",
        "del-cli": "^3.0.1",
        "dotenv": "^8.2.0",
        "html-minifier": "^4.0.0",
        "luxon": "^1.25.0",
        "markdown-it": "^12.0.2",
        "memfs": "^3.2.0",
        "memory-fs": "^0.5.0",
        "node-fetch": "^2.6.1",
        "npm-run-all": "^4.1.5",
        "sass": "^1.32.8",
        "svg-sprite": "^1.5.0",
        "webpack": "^4"
    },
    "devDependencies": {
        "cross-env": "^7.0.2"
    }
}
